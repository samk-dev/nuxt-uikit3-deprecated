{
  "name": "@samk_dev/nuxt-uikit3",
  "version": "1.0.0-beta-02",
  "description": "Nuxt Uikit 3 module",
  "workspaces": [
    "docs",
    "playground",
    "example"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "repository": "samk-dev/nuxt-uikit3",
  "homepage": "https://github.com/samk-dev/nuxt-uikit3",
  "author": {
    "name": "Sam K",
    "url": "https://samk.dev"
  },
  "bugs": "https://github.com/samk-dev/nuxt-uikit3/issues",
  "keywords": [
    "nuxt",
    "nuxtjs",
    "nuxt3",
    "nuxt-module",
    "uikit3",
    "yootheme"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "PORT=3666 nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,vue,md,json}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.3",
    "uikit": "^3.16.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.3",
    "@types/node": "^20.1.1",
    "@types/uikit": "^3.14.0",
    "changelogen": "^0.5.3",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.4.3",
    "prettier": "^2.8.8"
  },
  "lint-staged": {
    "src/**/*.{.js,ts,vue,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "test/**/*.{.js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "playground/**/*.{.js,ts,vue,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.11.0"
  },
  "packageManager": "npm@9.6.6",
  "publishConfig": {
    "access": "public"
  }
}
